{
  "name": "@skyfi-mcp/server",
  "version": "0.1.0",
  "description": "MCP server for SkyFi geospatial/satellite imagery services",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "skyfi-mcp": "dist/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:http": "MCP_TRANSPORT=http tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:http": "MCP_TRANSPORT=http node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "skyfi",
    "satellite",
    "geospatial",
    "imagery",
    "ai",
    "claude"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@skyfi-mcp/shared": "workspace:*",
    "axios": "^1.6.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "nock": "^14.0.10",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
